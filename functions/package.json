{
  "name": "aira-backend-functions",
  "version": "1.0.0",
  "description": "Firebase Cloud Functions for AIra Backend API",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "@mistralai/mistralai": "^1.12.0",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^4.22.1",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^6.0.0",
    "openai": "^6.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "typescript": "^5.9.3"
  },
  "private": true
}